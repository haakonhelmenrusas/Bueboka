{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "allowJs": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*",
        "./*"
      ]
    }
  },
  "extends": "expo/tsconfig.base",
  "exclude": [
    "node_modules"
  ]
}
